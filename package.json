{
  "name": "buger-eats",
  "version": "1.0.0",
  "description": "Projeto de teste front-end para o sistema buger eats",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --env allure=true",
    "ui-regression-allure": "cypress run --browser chrome --spec cypress/e2e/1-getting-started/**.js --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "keywords": [
    "cypress"
  ],
  "author": "Ana Carolina Cerqueira",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "allure": "0.0.0",
    "cypress": "^11.1.0",
    "cypress-file-upload": "^5.0.8",
    "faker": "^5.5.3"
  }
} 
